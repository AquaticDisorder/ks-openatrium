/**
 * Enable oa_variables module.
 */
function openatrium_update_7001() {
  if (!module_existS('oa_variables') && !module_enable(array('oa_variables'))) {
    trigger_error('Unable to enable oa_variables module. Please check for any missing depedencies.');
  }
}